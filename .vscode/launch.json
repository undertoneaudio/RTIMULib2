    "version": "0.2.0",
    "configurations": [
        {
			"name": "RTIMULib2 - Calc",
			"preLaunchTask": "Build RTIMULib2 - Calc",
			"type": "cppdbg",
			"request": "launch",
			"program": "${command:cmake.buildDirectory}/RTIMULibCal/RTIMULibCal",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
        {
			"name": "RTIMULib2 - Demo GL",
			"preLaunchTask": "Build RTIMULib2 - Calc",
			"type": "cppdbg",
			"request": "launch",
			"program": "${command:cmake.buildDirectory}/RTIMULibDemoGL/RTIMULibDemoGL",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
    ]
